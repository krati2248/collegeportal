<%- include('../header.ejs') %>
<div class="abcontai mb-4" style="height: 400px;padding-top: 160px;">
    <h1 class="text-center text-white" style="font-size:48px">Enrolled Courses</h1>
    <img src="./Images/slider-bottom-pattern.png" alt="/">
  
  </div>
<br>
<br>
 
 
<div class="container">
  

    <table class="table table-striped table-hover text-center">
         
        <tr>
             <caption>'</caption>
            
            <% if(mesg.length>0){ %>
                <div class="alert alert-success">
                    <!-- To print -->
                    <%= mesg %>
                </div>
                <% } %>
        </tr>
        <tr class="text-capitalize">
            <th>Id</th>

            <th>Name</th>
            <th>email</th>
            <th>Course</th>
            <th>Status</th>
            <th>Comment</th>
            <th>Action</th>
        </tr>
        <% c.forEach((item)=>{  %>
            <tr>
                <td><%= item._id %></td>
                <td><%= item.name %></td>
                <td><%= item.email %></td>
                <td><%= item.course %></td>
                <td><%= item.status %></td>
                <td><%= item.comment %></td>
                <td>
                    <a href="/courseView/<%= item._id %>" class="btn btn-primary">View</a>
                    <a href="/courseEdit/<%= item._id %>" class="btn btn-success">Edit</a>
                    <!-- <a href="/courseDelete/<%= item._id %>" class="btn btn-danger">Delete</a> -->
                </td>
            </tr>
        <%  }) %>

        
    </table>
   

</div>
<br>
<br>
<br>
<br>
 




<%-include('../footer.ejs')    %>